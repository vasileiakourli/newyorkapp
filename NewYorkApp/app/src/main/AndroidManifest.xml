<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="omada17.newyorkapp">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".EnterCoordinates"
            android:label="@string/title_activity_nymaps">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".InputDates"
            android:label="@string/title_activity_inputdates">

        </activity>

        <activity
            android:name=".FindBest"
            android:label="@string/title_findbest">

        </activity>

        <activity android:name=".LoadImage"
            android:label="@string/title_loadimage">

        </activity>

        <activity
            android:name=".NYMaps"
            android:label="@string/title_activity_nymaps">

        </activity>


    </application>

</manifest>
